{
    "manifest_version": 2,
    "name": "insaneificate",
    "version": "1.0",
  
    "description": "Adds a browser action icon to the toolbar. Click the button to choose an insaneification. The active tab's body content is then insanified.",
    "homepage_url": "https://github.com/skatingbread/insaneify",
    "icons": {
      "48": "icons/cyclone-48.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/cyclone-96.png",
      "default_title": "insaneify",
      "default_popup": "popup/choose_insane.html"
    },
  
    "web_accessible_resources": [
      "icons/spiral_icon.svg"
    ]
  }
  